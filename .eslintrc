{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    // "import/no-unresolved": [
    //   1,
    //   {
    //     "commonjs": true,
    //     "amd": true
    //   }
    // ],
    // "object-curly-newline": [
    //   "error",
    //   {
    //     "ObjectExpression": "always",
    //     "ObjectPattern": {
    //       "multiline": true
    //     },
    //     "ImportDeclaration": "never",
    //     "ExportDeclaration": {
    //       "multiline": true,
    //       "minProperties": 3
    //     }
    //   }
    // ],
    "object-curly-newline": [
      "off",
      {
        "ObjectExpression": "never",
        "ObjectPattern": "never",
        "ImportDeclaration": "never",
        "ExportDeclaration": {
          "multiline": true,
          "minProperties": 3
        }
      }
    ],
    "new-cap": [
      0,
      {
        "newIsCap": false
      }
    ],
    "no-plusplus": 0,
    "radix": [
      "warn",
      "as-needed"
    ],
    "max-len": 0,
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "func-names": [
      0,
      "as-needed"
    ],
    "space-before-function-paren": [
      1,
      "never"
    ],
    "template-curly-spacing": [
      "error",
      "always"
    ],
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "one-var": 1,
    "quotes": 0,
    "no-console": 0,
    "no-debugger": 1,
    "no-shadow": 0,
    "no-var": 1,
    "semi": [
      1,
      "always"
    ],
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 1,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "import/newline-after-import": 0,
    "no-param-reassign": [
      "off",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "^bar"
        ]
      }
    ]
  }
}